<androidx.cardview.widget.CardView xmlns:android="http://schemas.android.com/apk/res/android"
    android:layout_width="match_parent"
    android:layout_height="wrap_content"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_margin="7dp"
    android:background="@drawable/border_shandow">

    <LinearLayout
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:background="@color/white"
        android:orientation="vertical"
        android:padding="10dp">

        <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:orientation="horizontal">

            <ImageView
                android:id="@+id/imageVector"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_marginRight="10dp"
                android:src="@drawable/ic_user" />

            <TextView
                android:id="@+id/idProject"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:text="o"
                android:visibility="gone"
                android:src="@drawable/ic_trash" />

            <androidx.appcompat.widget.AppCompatTextView
                android:id="@+id/title"
                android:layout_width="220dp"
                android:layout_height="match_parent"
                android:text="@string/titleFeed"
                android:textColor="@color/black" />


            <ImageView
                android:id="@+id/DeleteProfileList"
                android:layout_width="55dp"
                android:layout_height="wrap_content"
                android:src="@drawable/ic_trash" />


            <ImageView
                android:id="@+id/editProfileList"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:src="@drawable/ic_edit" />
        </LinearLayout>

        <androidx.appcompat.widget.AppCompatTextView
            android:id="@+id/description"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:layout_marginTop="5dp"
            android:text="@string/descriptionFeed"
            android:textColor="@color/black" />

        <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:layout_marginTop="10dp"
            android:gravity="center_vertical"
            android:orientation="horizontal">

            <ImageView
                android:layout_width="53px"
                android:layout_height="wrap_content"
                android:src="@drawable/ic_mensage" />

        </LinearLayout>
    </LinearLayout>
</androidx.cardview.widget.CardView>
